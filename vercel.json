{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/health",
      "dest": "/api/webhook.py"
    }
  ],
  "functions": {
    "api/webhook.py": {
      "maxDuration": 30
    },
    "api/test.py": {
      "maxDuration": 10
    }
  }
}
